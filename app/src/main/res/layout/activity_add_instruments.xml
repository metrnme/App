<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.AddInstruments">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

    <TextView
        android:id="@+id/hdng_inst"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/pg_margin"
        android:layout_marginTop="@dimen/top_margin"
        android:layout_marginEnd="@dimen/pg_margin"
        android:fontFamily="@font/nunito_bold"
        android:text="@string/select_instruments"
        android:textSize="@dimen/hdng_txt_size" />

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>